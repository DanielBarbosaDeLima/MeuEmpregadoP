<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
 <navigation-rule>
		<from-view-id>/listaPacoteSeguro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>alterarPacoteSeguro</from-outcome>
			<to-view-id>/alterarPacoteSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/alterarPacoteSeguro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaPacoteSeguro</from-outcome>
			<to-view-id>/listaPacoteSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/contrato.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>contratoPacoteSeguro</from-outcome>
			<to-view-id>/contratoPacoteSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/contratoPacoteSeguro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaContratoSeguro</from-outcome>
			<to-view-id>/listaContratoSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/listaContratoSeguro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaPacoteSeguro</from-outcome>
			<to-view-id>/listaPacoteSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/listaPacoteSeguro.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>listaContratoSeguro</from-outcome>
			<to-view-id>/listaContratoSeguro.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<managed-bean>
		<managed-bean-name>EmpregadoManagedBean</managed-bean-name>
		<managed-bean-class>com.meuempregado.bean.EmpregadoManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>ContratoSeguroManagedBean</managed-bean-name>
		<managed-bean-class>com.meuempregado.bean.ContratoSeguroManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
</faces-config>
